<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.ictti.library.business.entity">
<class
name="ViewRecall"
table="viewrecall"
lazy="false"
>
<meta attribute="sync-DAO">false</meta>
<id
name="bookloanId"
type="integer"
column="book_loan_id"
>
<generator class="identity"/>
</id>
<property
name="userId"
column="user_id"
type="integer"
/>
<property
name="username"
column="user_name"
type="string"
/>
<property
name="email"
column="user_email"
type="string"
/>
<property
name="title"
column="title"
type="string"
/>
<property
name="Call_Number"
column="callnumber_id"
type="string"
/>
<property
name="ISBN"
column="ISBN_no"
type="string"
/>
<property
name="dueDate"
column="due_date"
type="date"
/>

</class>
</hibernate-mapping>
